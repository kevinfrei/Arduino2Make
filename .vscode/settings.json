{
  "search.exclude": {
    "public/**/*.js": true,
    "public/**/*.js.map": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules": true,
    "build": true,
    "dist": true,
    "yarn*.l*": true,
    "LICENSE*": true,
    ".*": true
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "prettier.configPath": ".prettierrc",
  "json.format.enable": false,
  "typescript.enablePromptUseWorkspaceTsdk": true
}
